package {{.Package}}

import (
	"fmt"
	"strings"
)

func (obj {{.Type}}) String() string {
	b := &strings.Builder{}
	b.WriteByte('{'){{range .Fields}}{{if not .First}}
	b.WriteByte(',')
	b.WriteByte(' '){{end}}
	_, _ = fmt.Fprintf(b, "%q:%v", "{{.Alias}}", obj.{{.Name}}){{end}}
	b.WriteByte('}')
	return b.String()
}
